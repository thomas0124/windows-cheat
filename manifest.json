{
    "manifest_version": 3,
    "name": "Windowsコマンドチートシート集",
    "description": "Windowsで扱うことのできるコマンドのチート集です。主に使いそうな物をまとめた為、全て網羅しているわけではございません",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "128": "images/icon128.png"
    }
}
